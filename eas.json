{
  "cli": {
    "version": ">= 18.0.1",
    "appVersionSource": "remote",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "env": {
        "EAS_SKIP_AUTO_FINGERPRINT": "1",
        "EXPO_PUBLIC_VIDEO_BACKEND_URL": "https://cookedapp.onrender.com"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
